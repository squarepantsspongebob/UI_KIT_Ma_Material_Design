/* MyDialog */
MyDialog{
    background-color: #e5e5e5;
    opacity: 56;
}
MyDialog QWidget#mainWidget{
    background-color: #99858585;
}
MyDialog QWidget#subWidget{
    border-radius: 10;
    background-color: #e5e5e5;
}
MyDialog QLabel#label1{
    color: black;
    font-size: 14;
}
MyDialog QLabel#label2{
    color: gray;
    font-size: 10;
    text-align: left;
}
MyDialog QPushButton{
    border: 0px;
    background-color: #e5e5e5;
    color: #661FFF;
}
MyDialog QPushButton:hover{
    background-color: #d8d3e6;
}
MyDialog QPushButton:pressed{
    background-color: #D0C0E6;
}

/* QRadioButton*/
QRadioButton{
    spacing: 14px;
}
QRadioButton::indicator{
    width: 20px;
    height: 20px;
}
QRadioButton::indicator::unchecked{
    image: url(":/pic/radio-button-unchecked.png") 18 18;
}

QRadioButton::indicator::checked{
    image: url(":/pic/radio-button-on.png");
}

/* QCheckBox */
QCheckBox::indicator::checked{
    image: url(":/pic/check_box-checked.png");
}
QCheckBox::indicator::unchecked{
    image: url(":/pic/check_box-unchecked.png");
}

