/* Global Variation */

Widget{
    background-color: #e5e5e5;
}

MyProgressBar {
      border: none;
      border-radius: 0px;
      text-align: center;
      color: transparent;   /* font color*/
      background: #EDE7F6;
      margin: 0;
  }

MyProgressBar::chunk {
      background: #661FFF;
      border-radius: 0px;
}


MyButton1{
    border: 1px solid #8E8E8E;
    border-radius: 4px;
    color: #661FFF;
    background-color: #e5e5e5;
}
MyButton1::hover{
    background-color: #EDE7F6;
}
MyButton1::pressed{
    background-color: #D1C4E9;
}
MyButton1::disabled{
    color: #969696;
    background-color: #CCCCCC;
}

MyButton1#pushButton_enabled{
    border: 0px solid #661FFF;
    border-radius: 4px;
    background-color: #661FFF;
    color: #e5e5e5;
}
MyButton1#pushButton_enabled::hover{
    background-color: #6E14EF;
}
MyButton1#pushButton_enabled::pressed{
    background-color: #873DF2;
}

/* Slider */
QSlider::groove:horizontal {
      background: #c6aee7;
      position: absolute; /* absolutely position 4px from the left and right of the widget. setting margins on the widget should work too... */
/*      left: 4px; right: 4px;*/
      height: 4;
      border: 0;
}
/* 14 + 1 + 1 = 16    16 / 2 = 8 */
QSlider::handle:horizontal {
      width: 14px;
      height: 14px;
      border: 1px solid #6200EE;
      border-radius: 8px;
      background: #6200EE;
      margin-top: -7px;
      margin-bottom: -7px;
}
