/* QTableView */
QTableView{
    background-color: #ffffff;
    selection-background-color: #F3EBFE;
    selection-color: #000000;
    gridline-color: #d8d3e6;
    font-size: 14px;
    border: 1px solid #d8d3e6;
    margin-left: -4;
    border-radius: 17px;
}
QTableView QScrollBar:horizontal {
    border: 0px solid red;
    border-radius: 5px;
    background-color: #d8d8d8;
    height: 10px;
/*    margin: 0px 0px 0 0px;*/
}
QTableView QScrollBar::handle:horizontal {
    background: #bebebe;
    min-width: 20px;
}
QTableView QScrollBar::add-line:horizontal {
    width: 0px;
}

QTableView QScrollBar::sub-line:horizontal {
    width: 0px;
}
QTableView QScrollBar:vertical {
    border: 0px solid red;
    border-radius: 5px;
    background-color: #d8d8d8;
    width: 10px;
}
QTableView QScrollBar::handle:vertical {
    background: #bebebe;
    min-height: 20px;
}
QTableView QScrollBar::add-line:vertical {
    height: 0px;
}

QTableView QScrollBar::sub-line:vertical {
    height: 0px;
}
QTableView QHeaderView::section{
    background-color: #ffffff;
    color: #797979;
    gridline-color: #d8d3e6;
    font-size: 13px;
    border: 1px solid #d8d3e6;
}
QTableView QHeaderView::section:checked{
}
QTableView QHeaderView::section:selected{
}
QTableView QCheckBox::indicator::checked{
    image: url(":/pic/check_box-checked.png");
}
QTableView QCheckBox::indicator::unchecked{
    image: url(":/pic/check_box-unchecked.png");
}
